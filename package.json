{
  "name": "simzoo",
  "version": "0.4.60",
  "description": "Package that contains the gym environments of the bayesian-learning-control package.",
  "keywords": [
    "reinforcement-learning",
    "simulation",
    "openai-gym",
    "gym-environments"
  ],
  "homepage": "https://github.com/rickstaa/simzoo#readme",
  "bugs": {
    "url": "https://github.com/rickstaa/simzoo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rickstaa/simzoo.git"
  },
  "license": "UNLICENSED",
  "author": "Rick Staa",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "changelog": "HUSKY=0 standard-version --skip.bump --skip.commit --skip.tag",
    "prepare": "husky install",
    "release": "HUSKY=0 standard-version",
    "sort": "sort-package-json"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "!(CHANGELOG)*.md": "./node_modules/.bin/remark . -o --",
    "!(**/(tests)/**)*.py": [
      "black",
      "flake8"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.0",
    "remark": "14.0.2",
    "remark-cli": "10.0.0",
    "remark-lint": "9.1.0",
    "remark-preset-lint-recommended": "6.1.1",
    "sort-package-json": "1.55.0",
    "standard-version": "9.3.2"
  }
}
